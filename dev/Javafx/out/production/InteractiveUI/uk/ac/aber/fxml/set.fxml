<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox>
               <children>
                  <HBox spacing="5.0">
                     <children>
                        <Label text="[x]" />
                        <Label text="Exercises" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label text="[x]" />
                        <Label text="Seconds each" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label text="[x]" />
                        <Label text="Second breaks" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label text="[x]" />
                        <Label text="Second half-time break" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Label text="[Total time]" StackPane.alignment="TOP_RIGHT" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
