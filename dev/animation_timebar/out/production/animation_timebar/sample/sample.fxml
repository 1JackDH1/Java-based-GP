                        <ProgressBar fx:id="sixthProgressBar" prefWidth="100.0" progress="0.0" translateX="-360" translateY="75" />
                        <ProgressBar fx:id="seventhProgressBar" prefWidth="100.0" progress="0.0" translateX="-380" translateY="75" />
                    </children>
                </HBox>


            </children>
        </VBox>
    </center>
