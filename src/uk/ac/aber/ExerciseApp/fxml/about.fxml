<VBox alignment="TOP_CENTER" maxWidth="-Infinity" prefHeight="640.0" prefWidth="640.0" spacing="5.0" styleClass="root" stylesheets="@style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">

   <children>
